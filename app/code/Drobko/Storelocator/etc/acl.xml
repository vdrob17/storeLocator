<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::content">
                    <resource id="Drobko_Storelocator::all" title="Store Locator" translate="title" sortOrder="10">
                        <resource id="Drobko_Storelocator::view" title="View Store Locator" translate="title" sortOrder="10"/>
                        <resource id="Drobko_Storelocator::edit" title="Edit Store Locator" translate="title" sortOrder="10"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Drobko_Storelocator::config_storelocator" title="Store Locator Settings" translate="title"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
